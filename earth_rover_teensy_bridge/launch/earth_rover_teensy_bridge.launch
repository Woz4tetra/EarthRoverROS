<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <group ns="earth_rover" >
        <node name="earth_rover_teensy_bridge" pkg="earth_rover_teensy_bridge" type="earth_rover_teensy_bridge_node" required="true" output="screen">
            <param name="serial_port" type="string" value="/dev/serial/by-id/usb-Teensyduino_USB_Serial_4989760-if00"/>
            <param name="serial_baud" type="int" value="115200"/>

            <!-- Sensor 1: Front right. 3.0cm -->
            <!-- Sensor 2: Front left. 3.0cm -->
            <!-- Sensor 3: Back right side. 2.5cm -->
            <!-- Sensor 4: Back left side. 2.5cm -->
            <!-- Sensor 5: Back right behind. 6.0cm -->
            <!-- Sensor 6: Back left behind. 6.0cm -->
            <rosparam param="activation_distances_cm">
                [
                    9.0,
                    9.0,
                    8.5,
                    8.5,
                    12.0,
                    12.0,
                ]
            </rosparam>
        </node>
    </group>
</launch>
