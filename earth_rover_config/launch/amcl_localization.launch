<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <arg name="map_name" default="map" />
    <arg name="map_path" default="$(find earth_rover_config)/maps/$(arg map_name)" />

    <node name="map_server" pkg="map_server" type="map_server" output="screen" args="-f $(arg map_path)"/>
</launch>
